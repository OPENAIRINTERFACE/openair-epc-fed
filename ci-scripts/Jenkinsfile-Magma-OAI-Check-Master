def magma_job = params.magma_job_name

pipeline {
  agent {
    label 'master'
  }
  stages {
    stage ('Trigger MAGMA pipeline') {
      steps {
        script {
          build wait: false, job: magma_job,
            parameters: [
              booleanParam(name: 'REGRESSION_TEST', value: true)
            ]
        }
      }
    }
  }
}
